package com.chainsys.file;
import java.io.File;
import java.io.IOException;
import java.io.PrintWriter;


public class FileOperations {
	public static void main(String[] args) {
		try {
		File file=new File("filename.txt");
		
		if(!file.exists()) {
			file.createNewFile();
		}
		}
	catch (IOException e) {
		e.printStackTrace();
	}
		
		
	}
}
	

	

